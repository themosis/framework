{
  "name": "themosis/framework",
  "description": "The Themosis framework.",
  "keywords": ["themosis", "framework", "wordpress"],
  "license": "GPL-2.0-or-later",
  "support": {
    "issues": "https://github.com/themosis/framework/issues",
    "source": "https://github.com/themosis/framework"
  },
  "authors": [
    {
      "name": "Julien LambÃ©",
      "email": "julien@themosis.com",
      "homepage": "https://www.themosis.com/"
    }
  ],
  "autoload": {
    "psr-4": {
      "Themosis\\": "src/"
    },
    "files": [
      "src/Core/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Themosis\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1.0",
    "composer/installers": "^1.0",
    "dragonmantank/cron-expression": "^2.2",
    "filp/whoops": "^2.1",
    "guzzlehttp/guzzle": "^6.3",
    "illuminate/auth": "^5.7",
    "illuminate/bus": "^5.7",
    "illuminate/cache": "^5.7",
    "illuminate/config": "5.7.*",
    "illuminate/console": "5.7.*",
    "illuminate/container": "5.7.*",
    "illuminate/cookie": "^5.7",
    "illuminate/database": "5.7.*",
    "illuminate/events": "5.7.*",
    "illuminate/filesystem": "5.7.*",
    "illuminate/hashing": "^5.7",
    "illuminate/http": "5.7.*",
    "illuminate/log": "5.7.*",
    "illuminate/mail": "^5.7",
    "illuminate/pagination": "^5.7",
    "illuminate/queue": "^5.7",
    "illuminate/routing": "5.7.*",
    "illuminate/session": "^5.7",
    "illuminate/support": "5.7.*",
    "illuminate/validation": "^5.7",
    "illuminate/view": "5.7.*",
    "league/flysystem": "^1.0",
    "league/fractal": "^0.17.0",
    "symfony/var-dumper": "^4.1",
    "twig/twig": " ^2.0",
    "vlucas/phpdotenv": "^2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "scripts": {
    "test": "phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
